<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cashify.details.DetailsFragment">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/sky"
        tools:context="com.cashify.Details.DetailsFragment"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="16dp"
            android:text="Zeitraum"
            android:textSize="30sp"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/textView9"
            android:layout_marginTop="75dp"
            app:layout_constraintTop_toBottomOf="@+id/toggleButton"
            android:layout_marginBottom="73dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:text="Details"
            android:textSize="30sp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="23dp"
            app:layout_constraintBottom_toBottomOf="@+id/category_spinner"
            android:layout_marginTop="66dp"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="70dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="23dp"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="0dp"
            android:layout_height="38dp"
            android:layout_alignLeft="@+id/ausgaben"
            android:layout_alignStart="@+id/ausgaben"
            android:layout_below="@+id/ausgaben"
            android:layout_toStartOf="@+id/editText2"
            android:text="Kategorie"
            android:textSize="24sp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="4dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView2"
            android:layout_marginEnd="4dp"
            app:layout_constraintRight_toRightOf="@+id/textView2"
            android:layout_marginTop="62dp"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="64dp"
            app:layout_constraintLeft_toLeftOf="@+id/textView2"
            app:layout_constraintTop_toTopOf="@+id/textView6"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="23dp"
            android:text="Ende"
            android:textSize="18sp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="23dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="23dp"
            android:text="Start"
            android:textSize="18sp"
            android:layout_marginLeft="23dp"
            app:layout_constraintLeft_toLeftOf="parent"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/textView9"
            android:layout_marginBottom="32dp" />

        <Spinner
            android:id="@+id/category_spinner"
            android:layout_width="206dp"
            android:layout_height="41dp"
            android:visibility="visible"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginEnd="16dp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/textView2"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/toggleButton"
            android:layout_marginBottom="23dp" />

        <EditText
            android:id="@+id/end_editText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="date"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/start_editText"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/start_editText"
            app:layout_constraintHorizontal_bias="0.0"
            tools:layout_constraintTop_creator="1"
            android:layout_marginTop="22dp"
            app:layout_constraintTop_toBottomOf="@+id/textView8" />

        <EditText
            android:id="@+id/start_editText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="date"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/end_editText"
            android:layout_marginStart="53dp"
            android:layout_marginEnd="54dp"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="24dp"
            app:layout_constraintLeft_toRightOf="@+id/textView8"
            android:layout_marginLeft="53dp"
            android:layout_marginRight="54dp"
            app:layout_constraintHorizontal_bias="0.0" />

        <Button
            android:id="@+id/months_3_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="18dp"
            android:text="3 MONTHS"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/end_editText"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1" />

        <Button
            android:id="@+id/months_1_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="1 MONTH"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            tools:layout_constraintTop_creator="1"
            android:layout_marginTop="18dp"
            app:layout_constraintTop_toBottomOf="@+id/end_editText" />

        <Button
            android:id="@+id/months_12_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:text="1 YEAR"
            app:layout_constraintBaseline_toBaselineOf="@+id/months_3_btn"
            app:layout_constraintLeft_toRightOf="@+id/months_3_btn"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintBaseline_creator="1" />

        <Button
            android:id="@+id/button_export"
            android:layout_width="368dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="48dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="39dp"
            android:text="EXPORT"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/months_1_btn"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintTop_creator="1" />

        <ToggleButton
            android:id="@+id/toggleButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ToggleButton"
            android:textOff="Einnahmen"
            android:textOn="Ausgaben"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginEnd="24dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginRight="24dp" />

        <EditText
            android:id="@+id/editText3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Name"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent" />

    </android.support.constraint.ConstraintLayout>

</FrameLayout>
